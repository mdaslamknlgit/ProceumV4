<?xml version="1.0"?> <configuration>   <configSections>     <sectionGroup name="Microsoft.Msn.Set.Web">       <section name="content" type="Microsoft.Msn.Set.Web.Configuration.WebContentConfiguration, Microsoft.Msn.Set.Web" allowLocation="true" allowDefinition="Everywhere"/>     </sectionGroup>   </configSections>   <Microsoft.Msn.Set.Web>      <content>        <handlers>         <add name="GetContent" method="GET" type="Microsoft.Msn.Set.Web.WebContentHandler, Microsoft.Msn.Set.Web" />         <add name="PostContent" method="POST" type="Microsoft.Msn.Set.Web.WebPostHandler, Microsoft.Msn.Set.Web" />       </handlers>        <webContent>          <!-- MSN Publishing Platform source (Bedrock) -->         <source name="Bedrock" baseUrl="/static/xml/">           <!-- Add virtual paths here.                  Virtual paths are custom content items that can contain one or multiple resources, and can have XSL Transforms applied automatically.                  -->           <cacheSettings duration="7200" fallback="false"/>                                 <contentItems>             <!-- Meta -->             <contentItem match="Meta/Default">               <processors>                 <add name="XsdValidator" type="Microsoft.Msn.Set.Web.Content.Xml.XsdValidationProcessor, Microsoft.Msn.Set.Web">                   <xsd url="xsd/page.xsd" />                 </add>                 <add name="Xslt" type="Microsoft.Msn.Set.Web.Content.Xml.XsltProcessor, Microsoft.Msn.Set.Web">                   <xslt url="xslt/MetaDefault.xslt">                   </xslt>                 </add>               </processors>               <resources>                 <add name="Page" url="Page.xml" />               </resources>             </contentItem>                          <!-- End Meta -->              <!-- Videos -->             <contentItem match="PromoVideoPlayerHTML/(?'id'\w+)" regex="true">               <merger name="Merger" type="Microsoft.Msn.Set.Web.Content.Xml.XmlSequentialMerger, Microsoft.Msn.Set.Web"/>               <processors>                 <add name="XsltProcessor" type="Microsoft.Msn.Set.Web.Content.Xml.XsltProcessor, Microsoft.Msn.Set.Web">                   <xslt url="xslt/PromoVideoPlayerHTML.xslt">                     <xsltArgs>                       <add name="videoId" value="${id}"/>                     </xsltArgs> 		    <xsltExtensions>                       <add namespace="Proceum" type="Proceum.XsltExtensions, Proceum" />                     </xsltExtensions>                   </xslt>                 </add>               </processors>               <resources>                 <add name="PromoVideo" url="PromoVideo.xml" />                 <add name="Brads" url="Brads.xml" />               </resources>             </contentItem>              	      <contentItem match="VerticalPlaylist/(?'id'\w+)" regex="true">               <processors>                 <add name="XsltProcessor" type="Microsoft.Msn.Set.Web.Content.Xml.XsltProcessor, Microsoft.Msn.Set.Web">                   <xslt url="xslt/VerticalPlaylistModule.xslt">                     <xsltArgs>                       <add name="videoId" value="${id}"/>                     </xsltArgs>                    </xslt>                 </add>               </processors>               <resources>                 <add name="PromoVideo" url="PromoVideo.xml" />               </resources>             </contentItem>             <!-- End Videos -->               <!-- Share -->             <contentItem match="ShareView/(?'id'\w+)" regex="true">               <merger name="Merger" type="Microsoft.Msn.Set.Web.Content.Xml.XmlSequentialMerger, Microsoft.Msn.Set.Web"/>               <processors>                 <add name="XsltProcessor" type="Microsoft.Msn.Set.Web.Content.Xml.XsltProcessor, Microsoft.Msn.Set.Web">                   <xslt url="xslt/ShareView.xslt" enableScript="true">                     <xsltArgs>                       <add name="section" value="videos"/>                       <add name="baseUrl" value="${BaseUrl}"/>                       <add name="videoId" value="${id}"/>                     </xsltArgs>                     <xsltExtensions>                       <add namespace="Proceum" type="Proceum.XsltExtensions, Proceum" />                     </xsltExtensions>                   </xslt>                 </add>               </processors>               <resources>                 <add name="PromoVideo" url="PromoVideo.xml" />               </resources>             </contentItem>                          <!-- End Share -->                                                    <!-- Site Common-->             <contentItem match="SiteHeader">               <!--<cache duration="0" />-->               <processors>                 <add name="XsdValidator" type="Microsoft.Msn.Set.Web.Content.Xml.XsdValidationProcessor, Microsoft.Msn.Set.Web">                   <xsd url="xsd/Page.xsd" />                 </add>                 <add name="XsltProcessor" type="Microsoft.Msn.Set.Web.Content.Xml.XsltProcessor, Microsoft.Msn.Set.Web">                   <xslt url="xslt/SiteHeader.xslt" />                 </add>               </processors>               <resources>                 <add name="page" url="Page.xml" />               </resources>             </contentItem>              <contentItem match="SiteFooter">               <!--<cache duration="0" />-->               <processors>                 <add name="XsdValidator" type="Microsoft.Msn.Set.Web.Content.Xml.XsdValidationProcessor, Microsoft.Msn.Set.Web">                   <xsd url="xsd/Page.xsd" />                 </add>                 <add name="XsltProcessor" type="Microsoft.Msn.Set.Web.Content.Xml.XsltProcessor, Microsoft.Msn.Set.Web">                   <xslt url="xslt/SiteFooter.xslt" />                 </add>               </processors>               <resources>                 <add name="page" url="Page.xml" />               </resources>             </contentItem>              <contentItem match="MsnFooter">               <processors>                 <add name="XsltProcessor" type="Microsoft.Msn.Set.Web.Content.Xml.XsltProcessor, Microsoft.Msn.Set.Web">                   <xslt url="xslt/MsnFooter.xslt">                   </xslt>                 </add>               </processors>               <resources>                 <add name="footer" url="Page.xml">                 </add>               </resources>             </contentItem>                          <!-- End Site Common -->            </contentItems>          </source>          <!-- MSN source for standard MSN components (e.g., header) -->         <source name="MSN" baseUrl="http://msn.com">           <cacheSettings duration="7200" fallback="true"/>           <contentItems>             <contentItem match="MsnChannels">               <cache fallback="true"/>               <processors>                 <add name="XsltProcessor" type="Microsoft.Msn.Set.Web.Content.Xml.XsltProcessor, Microsoft.Msn.Set.Web">                   <xslt url="xslt/MsnChannels.xslt"></xslt>                 </add>               </processors>               <resources>                 <add name="channels" url="rss/crossnavfeed.aspx"/>               </resources>             </contentItem>           </contentItems>         </source>       </webContent>            </content>    </Microsoft.Msn.Set.Web>  </configuration>
